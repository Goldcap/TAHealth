/* CSS Document */
body{ padding:0; }
#extra{ position:relative; right:<del>60px; }
#container{ margin:0 30px; }
#centerdiv { float: left; padding-right: 150px;}
#div03{
  float: left;
  position: relative;
  width: 180px;          /* LC width */
  padding: 0 10px;       /* LC padding */
  right: 200px;          /* LC fullwidth + CC padding */
  margin-left: -100%;
  padding-left: 50px;
}


@charset "UTF-8";
/* CSS Document */

#Page {
	position: relative;
}
.tnf-logo img,
.Martin-Mull-image img {
	display:block;
}
.Martin-Mull-image {
	margin-right: -11px;	
}

#Navbar a {
	background: url(/images/btn-Nav.png) no-repeat bottom left;
}
#Navbar li {position:relative}

.home .three-panels .panel-two,
.home .three-panels .panel-three {
	background:url(/images/bg-panel-arrows-279x193.png) no-repeat top left;
}
#Footer {
	background: url(/images/blueprint-ie.gif) no-repeat center 0;
	zoom: 1;
}
#Footer .content {
	background: none;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/bg-footer-trans.png', sizingMethod='scale');
	zoom: 1;
}
#Footer .content a {
	zoom: 1;
}

.aboutBlurb {
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/aboutBlurb-trans.png', sizingMethod='scale');
	height: 15px;
}
.aboutBlurb a {
	background: none;
	position: relative;
	zoom: 1;
}

/* accordian */
.home .three-panels .panel-one {
	display: inline;
	background: url(/images/bg-panel-279x193.png) no-repeat left top;
}

.home .two-panels .panel-one {
	display: inline;
}

/* slide-box */
ul.slide-content {
	zoom: 1;
}
ul.slide-content img,
a.thumb-link img {
	background: 0;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/VideoThumbs/playOverlay-trans.png', sizingMethod='scale');
}
ul.slide-content img.fake,
a.thumb-link img.fake {
	background: 0;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/VideoThumbs/playOverlayFake-tran.png', sizingMethod='scale');
}
ul.slide-content li {
	display: inline;
}

#ImageViewer .ie-sux {
	height: 100%;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/ImageViewerBG-trans.png', sizingMethod='scale');
}
a.thumb-link .thumbnail {
	display: inline;
}
#ImageViewer {
	position: absolute;
	background: none;
}
.next-prev-btns {
	margin-left: 0;
}

.process .right-hand .step1 a {
	background: none;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/btn-processTools-step01-trans.png', sizingMethod='scale');
}
.process .right-hand .step2 a {
	background: none;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/btn-processTools-step02-trans.png', sizingMethod='scale');
}
.process .right-hand .step3 a {
	background: none;
	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/images/btn-processTools-step03-trans.png', sizingMethod='scale');
}

.right-hand {
	margin-right: -60px;
}
.right-hand .steps a {
	position: relative;
	cursor: pointer;
}
.process .right-hand .step1 a:hover img,
.process .right-hand .step2 a:hover img,
.process .right-hand .step3 a:hover img {
	visibility: hidden;
}
