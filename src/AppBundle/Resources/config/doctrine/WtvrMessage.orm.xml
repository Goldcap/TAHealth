<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\WtvrMessage" table="wtvr_message">
    <id name="wtvrMessageId" type="integer" column="wtvr_message_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="wtvrMessageIdentifier" type="string" column="wtvr_message_identifier" length="255" nullable="true"/>
    <field name="wtvrMessageScope" type="string" column="wtvr_message_scope" length="25" nullable="true"/>
    <field name="wtvrMessageSendUserId" type="integer" column="wtvr_message_send_user_id" nullable="true"/>
    <field name="wtvrMessageSendEmail" type="string" column="wtvr_message_send_email" length="255" nullable="true"/>
    <field name="wtvrMessageSendFname" type="string" column="wtvr_message_send_fname" length="255" nullable="true"/>
    <field name="wtvrMessageSendLname" type="string" column="wtvr_message_send_lname" length="255" nullable="true"/>
    <field name="wtvrMessageSubject" type="string" column="wtvr_message_subject" length="255" nullable="true"/>
    <field name="wtvrMessageBody" type="text" column="wtvr_message_body" length="65535" nullable="true"/>
    <field name="wtvrMessageText" type="text" column="wtvr_message_text" length="65535" nullable="true"/>
    <field name="wtvrMessageDate" type="datetime" column="wtvr_message_date" nullable="true"/>
    <field name="wtvrMessageTemplate" type="string" column="wtvr_message_template" length="50" nullable="true"/>
  </entity>
</doctrine-mapping>
