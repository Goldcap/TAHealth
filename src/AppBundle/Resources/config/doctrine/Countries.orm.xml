<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Countries" table="countries">
    <id name="countryId" type="integer" column="country_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="countryName" type="string" column="country_name" length="255" nullable="false"/>
    <field name="countryCode" type="string" column="country_code" length="2" nullable="true"/>
    <field name="sortVar" type="integer" column="sort_var" nullable="false"/>
  </entity>
</doctrine-mapping>
